{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@core/mongo/(.*)": "<rootDir>/../libs/mongo/src/$1",
    "@core/mongo": "<rootDir>/../libs/mongo/src",
    "@core/sql/(.*)": "<rootDir>/../libs/sql/src/$1",
    "@core/sql": "<rootDir>/../libs/sql/src",
    "@core/common/(.*)": "<rootDir>/../libs/common/src/$1",
    "@core/common": "<rootDir>/../libs/common/src"
  }
}